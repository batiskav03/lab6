<StackPane fx:id="parentContainer" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.ClientAuthorization">

    <AnchorPane id="AnchorPane" fx:id="anchorRoot" prefHeight="1080.0" prefWidth="1920.0" style="-fx-background-color: #EED8C9;">

        <Group>
            <VBox />
        </Group>
        <Button fx:id="signupButton" layoutX="822.0" layoutY="565.0" mnemonicParsing="false" onAction="#tryToSignup" prefHeight="94.0" prefWidth="274.0" style="-fx-background-color: #B38867; -fx-background-radius: 350;" text="%signupButton" textFill="#eed8c9">
            <font>
                <Font name="Segoe UI Black" size="42.0" />
            </font>
        </Button>
        <Button fx:id="loginButton" layoutX="823.0" layoutY="717.0" mnemonicParsing="false" onAction="#loadLogin" prefHeight="89.0" prefWidth="274.0" style="-fx-background-color: #B38867; -fx-background-radius: 350;" text="%loginButton" textFill="#eed8c9">
            <font>
                <Font name="Segoe UI Black" size="42.0" />
            </font>
        </Button>
        <Label layoutX="644.0" layoutY="315.0" textFill="#42313a">
            <font>
                <Font name="Segoe UI Black" size="41.0" />
            </font>
        </Label>
        <Label layoutX="582.0" layoutY="333.0" text="%plsAuth" textFill="#42313a">
            <font>
                <Font name="Segoe UI Black" size="48.0" />
            </font>
        </Label>
        <Label layoutX="691.0" layoutY="169.0" text="%hellouser" textFill="#42313a">
            <font>
                <Font name="Segoe UI Black" size="80.0" />
            </font>
        </Label>
        <ImageView fitHeight="223.0" fitWidth="236.0" layoutX="1693.0" layoutY="-7.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../GUI/Data/93533501-53aa0d80-f943-11ea-90d1-e6e70eca2e29.gif" />
            </image>
        </ImageView>
        <ImageView fitHeight="656.0" fitWidth="626.0" layoutX="18.0" layoutY="424.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../GUI/Data/ZAc2.gif" />
            </image>
        </ImageView>
        <MenuButton layoutX="56.0" layoutY="133.0" mnemonicParsing="false" prefHeight="59.0" prefWidth="169.0" style="-fx-background-color: #B38867; -fx-background-radius: 20;" text="Change language" textFill="#eed8c9">
            <items>
                <MenuItem fx:id="en" mnemonicParsing="false" onAction="#en" text="en" />
                <MenuItem fx:id="fr" mnemonicParsing="false" onAction="#fr" text="fr" />
            <MenuItem fx:id="ru" mnemonicParsing="false" onAction="#ru" text="ru" />
            <MenuItem fx:id="cs" mnemonicParsing="false" onAction="#cs" text="cs" />
            </items>
            <font>
                <Font name="Segoe UI Bold" size="15.0" />
            </font>
        </MenuButton>
        <Button fx:id="themeButton" layoutX="1748.0" layoutY="52.0" mnemonicParsing="false" onAction="#switchToAppDark" opacity="0.0" prefHeight="103.0" prefWidth="114.0" style="-fx-background-radius: 12323;" />
        <cursor>
            <Cursor fx:constant="DEFAULT" />
        </cursor>

    </AnchorPane>
</StackPane>
