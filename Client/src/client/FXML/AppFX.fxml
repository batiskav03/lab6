<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<?import javafx.scene.layout.StackPane?>
<StackPane fx:id="parentContainer" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="client.ClientAuthorization">
    <AnchorPane id="AnchorPane" fx:id="anchorRoot" prefHeight="444.0" prefWidth="720.0"
                style="-fx-background-color: #2F4F4F; -fx-border-color: #bababa; -fx-border-width: 10;">

        <Group>
            <VBox/>
        </Group>
        <ToolBar layoutX="493.0" layoutY="336.0" prefHeight="47.0" prefWidth="130.0">
            <items>
                <Button fx:id="loginButton" mnemonicParsing="false" onAction="#loadLogin" text="Log In"/>
                <Button fx:id="signupButton" layoutX="10.0" layoutY="13.0" mnemonicParsing="false" text="Sign Up" onAction="#tryToSignup"/>
            </items>
        </ToolBar>
        <cursor>
            <Cursor fx:constant="DEFAULT"/>
        </cursor>

    </AnchorPane>
</StackPane>