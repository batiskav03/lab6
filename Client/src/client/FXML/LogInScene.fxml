<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<StackPane fx:id="parentContainer" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="client.ClientAuthorization">
<AnchorPane prefHeight="444.0" prefWidth="720.0" style="-fx-background-color: #ACBEBE;">
    <PasswordField fx:id="passwordField" layoutX="248.0" layoutY="237.0" prefHeight="26.0" prefWidth="222.0" promptText="Password" style="-fx-background-color: #1995ad;">
        <font>
            <Font name="Segoe UI Bold" size="12.0" />
        </font></PasswordField>
    <Group>
        <VBox />
    </Group>
    <TextField fx:id="loginField" layoutX="249.0" layoutY="196.0" prefHeight="25.0" prefWidth="221.0" promptText="Login" style="-fx-background-color: #1995ad;">
        <font>
            <Font name="Segoe UI Bold" size="12.0" />
        </font>
        <cursor>
            <Cursor fx:constant="WAIT" />
        </cursor></TextField>
    <Label layoutX="188.0" layoutY="62.0" prefHeight="74.0" prefWidth="343.0" text="Authorization" textFill="#a01d26">
        <font>
            <Font name="Segoe UI Black Italic" size="50.0" />
        </font>
    </Label>
    <Button fx:id="enterToSystem" layoutX="248.0" layoutY="299.0" mnemonicParsing="false" onAction="#enterToSystem" prefHeight="53.0" prefWidth="221.0" style="-fx-background-color: #A01D26;" text="Enter" textFill="#f4f4ef">
        <font>
            <Font name="Segoe UI Black" size="24.0" />
        </font>
    </Button>
   <Button fx:id="backButton" layoutX="605.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" prefHeight="32.0" prefWidth="93.0" style="-fx-background-color: #A01D26;" text="back" textFill="#f4f4ef">
      <font>
         <Font name="Segoe UI Black" size="24.0" />
      </font>
   </Button>
    <cursor>
        <Cursor fx:constant="DEFAULT" />
    </cursor>

</AnchorPane>
</StackPane>