<?import javafx.scene.text.Font?>

<StackPane fx:id="parentContainer" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.SenderManager">
    <AnchorPane id="AnchorPane" fx:id="anchorRoot" prefHeight="108.0" prefWidth="1920.0" style="-fx-background-color: #2F4F4F; -fx-border-color: #bababa; -fx-border-width: 10;">
