                      <font>
                          <Font name="Segoe UI Black" size="32.0" />
                      </font>
                  </Button>
               <MenuButton fx:id="filterByColorButton" mnemonicParsing="false"  prefHeight="75.0" text="FILTER BY COLOR">
                 <items>
                   <MenuItem fx:id="blackFilter" mnemonicParsing="false" onAction="#blackFilter" text="BLACK" />
                   <MenuItem id="yellowFilter" fx:id="yellowFilter" mnemonicParsing="false" onAction="#yellowFilter" text="YELLOW" />
